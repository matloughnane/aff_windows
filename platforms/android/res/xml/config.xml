<?xml version='1.0' encoding='utf-8'?>
<widget id="com.matloughnane.arranmoreff" version="0.0.1" xmlns="http://www.w3.org/ns/widgets" xmlns:cdv="http://cordova.apache.org/ns/1.0">
    <preference name="loglevel" value="DEBUG" />
    <feature name="NetworkStatus">
        <param name="android-package" value="org.apache.cordova.networkinformation.NetworkManager" />
    </feature>
    <name>Android Ferry</name>
    <description>
        The Arranmore Fast Ferry app, the fastest way to get all the latest ferry times.
    </description>
    <author email="mat.loughnane@gmail.com" href="www.matloughnane.com">
        Mat Loughnane
    </author>
    <content src="index.html" />
    <access launch-external="yes" origin="tel:*" />
    <access origin="*" />
    <icon src="res/arranmoreff_icon.png" />
</widget>
